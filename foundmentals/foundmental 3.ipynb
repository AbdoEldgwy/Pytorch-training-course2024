   "source": [
    "x = torch.rand(5, 3)\n",
    "print(x,\"\\n\")\n",
    "\n",
    "# 1:\n",
    "transposed_tensor = x.transpose(0, 1)\n",
    "print(transposed_tensor)\n",
    "\n",
    "# 2:\n",
    "transposed_tensor = x.t()\n",
    "print(\"\\n\",transposed_tensor)"
